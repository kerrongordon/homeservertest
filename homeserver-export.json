{
  "title": "homeserver",
  "containers": [
    {
      "title": "transmission",
      "name": "transmission",
      "image": "linuxserver/transmission",
      "description": "",
      "categories": [
        "Downloads"
      ],
      "platform": "linux",
      "note": "transmission web torrent ",
      "restart_policy": "unless-stopped",
      "ports": [
        {
          "label": "WebUi",
          "hport": "9091",
          "cport": "9091",
          "proto": "tcp"
        },
        {
          "label": "system",
          "hport": "51413",
          "cport": "51413",
          "proto": "tcp"
        },
        {
          "label": "usystem",
          "hport": "51413",
          "cport": "51413",
          "proto": "udp"
        }
      ],
      "volumes": [
        {
          "container": "/config",
          "bind": "/transmission/config",
          "variable": "!config"
        },
        {
          "container": "/downloads",
          "bind": "/transmission/downloads",
          "variable": "!downloads"
        },
        {
          "container": "/watch",
          "bind": "/transmission/watch",
          "variable": "!data"
        }
      ],
      "env": [
        {
          "name": "PUID",
          "default": "!PUID",
          "label": "PUID"
        },
        {
          "name": "PGID",
          "default": "!PGID",
          "label": "PGID"
        },
        {
          "name": "TZ",
          "default": "!TZ",
          "label": "TZ"
        }
      ],
      "command": [],
      "devices": [],
      "labels": [],
      "sysctls": [],
      "cap_add": [],
      "logo": "https://raw.githubusercontent.com/dperson/transmission/master/logo.png",
      "network_mode": "host"
    }
  ]
}